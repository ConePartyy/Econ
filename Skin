local ReplicatedStorage = game:GetService("ReplicatedStorage")
local animlib = loadstring(game:HttpGet('https://raw.githubusercontent.com/ConePartyy/Econ/refs/heads/main/Anim'))()

local mod = {
	viewmodels = {},
	viewmodelorder = {}
}

_G.animLib = animlib

local qtbl = {}
local function add_viewmodel(name, icon, iconhigh, imagecenter, elimImg, elimImgScale, rootOffset, _equip, _idle, _sprint, _inspect, _rareinspect, itemSpecAnims, infoTbl)
	local itbl = {
		image = icon,
		imagehighres = iconhigh,
		imagecentered = imagecenter,
		elimfeedimage = elimImg,
		elimfeedimagescale = elimImgScale,
		rootoffset = rootOffset or CFrame.identity,
		anims = {
			equip = _equip,
			idle = _idle,
			sprint = _sprint,
			inspect = _inspect,
			rareinspect = _rareinspect,
		},
		
		animsReverse = {
			_equip = 'equip',
			_idle = 'idle',
			_sprint = 'sprint',
			_inspect = 'inspect',
			_rareinspect = 'rareinspect'
		},
	}
	
	-- // Adding Item-Specific Animations (Shooting, Reload, etc)
	
	if itemSpecAnims then
		for name, anim in pairs(itemSpecAnims) do
			itbl.animsReverse[anim] = name
			itbl.anims[name] = anim
		end
	end
	
	-- // Checking Animation Availability
	
	local p1, p2, p3 = pairs(itbl.anims)
	for _, anim in pairs(itbl.anims) do
		local var81 = animlib.Info[anim]
		assert(var81, 'bro your animation doesnt exist: '..anim)
	end
	
	-- // Adding Extra Item Info

	if infoTbl then
		for set, val in pairs(infoTbl) do
			itbl[set] = val
		end
	end
	
	-- // Finish

	mod.viewmodels[name] = itbl
	table.insert(mod.viewmodelorder, name)
	
	-- // Test Existing

	if qtbl[icon] and not string.find(icon, '_') then
		warn("Image already exists:", name)
	end

	if qtbl[iconhigh] and not string.find(iconhigh, '_') then
		warn("ImageHighResolution already exists:", name)
	end

	if elimImg and qtbl[elimImg] and not string.find(elimImg, '_') then
		warn("EliminationFeedImage already exists:", name)
	end

	qtbl[icon] = true
	qtbl[iconhigh] = true
	if elimImg then
		qtbl[elimImg] = true
	end
end

