local mod = {
	Info = {},
	InfoReverse = {}
}

local relAnim = game:GetService('ReplicatedStorage').Modules.AnimationLibrary

local function add_animation(name, id, length, speed, idkserver, stamp, extraInfo) 
	speed = speed or 1
	
	local rel = relAnim:FindFirstChild(name) or nil
	
	mod.Info[name] = {
		AnimationID = id,
		Speed = speed,
		Length = (length and length / speed),
		ActionTimestamp = (stamp and stamp / speed),
		SoundCallback = rel and require(rel),
		ExtraInformation = extraInfo
	}
	
	if #string.split(name, '_') == 1 then
		mod.InfoReverse[id] = {
			Name = name,
			Speed = speed,
			Length = (length and length / speed),
			ActionTimestamp = (stamp and stamp / speed),
			SoundCallback = rel and require(rel),
			ExtraInformation = extraInfo
		}
	end
end

